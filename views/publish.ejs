<body>
  <div class="container">
    <h1>Publish Information</h1>
    <form id="publishForm" method="post" action="/publish">
      <label for="college"><b>College Name:</b></label>
      <input type="text" id="college" name="college"><br>
      
      <label for="course"><b>Course:</b></label>
      <input type="text" id="course" name="course"><br>
      
      <label for="semester"><b>Semester:</b></label>
      <input type="text" id="semester" name="semester"><br>
      
      <label for="subject"><b>Subject:</b></label>
      <input type="text" id="subject" name="subject"><br>
      
      <label for="topic"><b>Topic:</b></label>
      <input type="text" id="topic" name="topic"><br>

      <label for="youtubelink"><b>Youtube Link:</b></label>
      <input type="text" id="youtubelink" name="youtubelink"><br>

      <label for="bookslink"><b>Books Link:</b></label>
      <input type="text" id="bookslink" name="bookslink"><br>
      
      <button type="submit">Submit</button>
    </form>
